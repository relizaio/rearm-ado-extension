{
    "manifestVersion": 1,
    "id": "rearm-cli-tasks",
    "name": "ReARM CLI Tasks",
    "version": "1.1.1",
    "publisher": "Reliza",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Azure DevOps extension to download and setup ReARM CLI and submit build artifacts and metadata to ReARM",
    "public": true,
    "galleryFlags": ["Public"],
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "images/icon.png"
    },
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "files": [
        {
            "path": "tasks/RearmCliInstall"
        },
        {
            "path": "tasks/RearmReleaseInitialize"
        },
        {
            "path": "tasks/RearmReleaseFinalize"
        }
    ],
    "contributions": [
        {
            "id": "rearm-cli-install",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/RearmCliInstall"
            }
        },
        {
            "id": "rearm-release-initialize",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/RearmReleaseInitialize"
            }
        },
        {
            "id": "rearm-release-finalize",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "tasks/RearmReleaseFinalize"
            }
        }
    ],
    "repository": {
        "type": "git",
        "uri": "https://github.com/relizaio/rearm-ado-extension"
    }
}
